# platform :ios, '9.0'

target 'AppUIKit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AppUIKit
  pod 'IUExtensions'

  target 'AppUIKitTests' do
    inherit! :search_paths
  end
end

